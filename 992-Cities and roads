#include <iostream>
using namespace std;

int main(){
    int n;
    cin >> n;
    int count=0;
    int matrix[n][n];
    for(int i=0;i<n;++i){
        for(int j=0;j<n;++j){
            cin >> matrix[i][j];
            if(matrix[i][j]==1){
                count++;
            }
        }
    }
    cout << count/2 << endl;
    return 0;
}
