#include <iostream>
#include <cmath>
using namespace std;

int main(){
    int n;
    cin >> n;
    int t=0;
    for(int i=2;i<sqrt(n);i++){
        if(n%i==0){
            t=1;
            break;
        }
    }
    if(t==0) cout << "Yes" << endl;
    else cout << "No" << endl;
    return 0;
    
}
