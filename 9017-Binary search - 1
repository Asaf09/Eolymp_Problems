#include <iostream>
#include <algorithm>
#include <vector>

using namespace std;

int countOccurrences(vector<int>& arr, int x) {
    int count = 0;
    int left = 0;
    int right = arr.size() - 1;

    while (left <= right) {
        int mid = left + (right - left) / 2;

        if (arr[mid] == x) {
            count++;
            int i = mid - 1;
            while (i >= left && arr[i] == x) {
                count++;
                i--;
            }
            int j = mid + 1;
            while (j <= right && arr[j] == x) {
                count++;
                j++;
            }
            return count;
        } else if (arr[mid] < x) {
            left = mid + 1;
        } else {
            right = mid - 1;
        }
    }
    return count;
}

int main() {
    int n, q;
    cin >> n >> q;
    int num;
    vector<int> v;
    v.resize(n); // Boyutu n olarak ayarlayÄ±n

    for (int i = 0; i < n; i++)
        cin >> v[i];

    for (int i = 0; i < q; i++) {
        cin >> num;
        cout << countOccurrences(v, num) << endl;
    }

    return 0;
}
